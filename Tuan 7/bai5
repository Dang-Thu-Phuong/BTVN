#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <set>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int q;
    cin>>q;
    set<int> s;
    while(q--){
        int t,x;
        cin>>t>>x;
        if(t==1){
            s.insert(x);
        }else
        if(t==2){
            s.erase(x);
            
        }else
        if(t==3){
            if(s.find(x)!=s.end()){
                cout<<"Yes"<<endl;
            }else{
                cout<<"No"<<endl;
            }
        }
    }
      
    return 0;
}
