#include<iostream>
#include<vector>
using namespace std;
int binarySearch(vector<int> & arr , int left, int right, int target){
    if(left>right){
        return -1;
    }
    int mid=left+(right-left)/2;
    if(arr[mid]==target){
        return arr[mid];
    }
    else if(arr[mid]>target){
        return binarySearch(arr,left, mid-1,target);
    }
    else if(arr[mid]<target)
{
    return binarySearch(arr,mid+1,right,target);
}}
int main(){
    int n;
    cin>>n;
    vector<int> arr(n);
    for(int i=0;i<n;++i){
        cin>>arr[i];
    }
    int target;
    cin>>target;
    int left=0;
    int right=arr.size()-1;
    
    int resul=binarySearch(arr,left,right,target);
    cout<<resul<<endl;
    return 0;
}
